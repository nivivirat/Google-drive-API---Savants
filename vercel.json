{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/images/day1", "dest": "/app.js" },
    { "src": "/api/images/day2", "dest": "/app.js" },
    { "src": "/api/images/carousel", "dest": "/app.js" },
    {
      "src": "/(.*)",
      "dest": "/client/dist/index.html"
    }
  ]
}
